{
  "title": "Fine volume control",
  "rules": [
    {
      "description": "map hyper + volume to finer adjustments",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f11",
            "modifiers": {
              "mandatory": [
                      "command",
                      "shift",
                      "option",
                      "control"
              ]
            }
          },
          "to": [
            {
              "key_code": "volume_decrement",
              "modifiers": [
                "left_shift",
                "left_option"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f12",
            "modifiers": {
              "mandatory": [
                      "command",
                      "shift",
                      "option",
                      "control"
              ]
            }
          },
          "to": [
            {
              "key_code": "volume_increment",
              "modifiers": [
                "left_shift",
                "left_option"
              ]
            }
          ]
        }
      ]
    }
  ]
}
